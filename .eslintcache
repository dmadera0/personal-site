             item.selected = false;\n            }\n        });\n\n        this.setState({\n            items\n        });\n\n    }\n\n    makeItems = (items) => {\n        return items.map(item => {\n            return <Card item={item} click={(e => this.handleCardClick(item.id, e))} key={item.id} />\n        })\n    }\n\n    render(){\n        return(\n            <Container fluid={true}>\n                <Row className=\"justify-content-around\">\n                    {this.makeItems(this.state.items)}\n                </Row>\n            </Container>\n        );\n    }\n\n}\n\nexport default Carousel;",["64","65"],"/Users/dmadera/learn/MOD6/personal_site/personal-website/src/components/Card.js",[],"/Users/dmadera/learn/MOD6/personal_site/personal-website/src/pages/AboutPage.js",[],"/Users/dmadera/learn/MOD6/personal_site/personal-website/src/components/Content.js",[],"/Users/dmadera/learn/MOD6/personal_site/personal-website/src/components/CardInfo.js",[],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":1,"column":17,"nodeType":"72","messageId":"73","endLine":1,"endColumn":26},{"ruleId":"70","severity":1,"message":"74","line":7,"column":8,"nodeType":"72","messageId":"73","endLine":7,"endColumn":19},{"ruleId":"66","replacedBy":"75"},{"ruleId":"68","replacedBy":"76"},{"ruleId":"66","replacedBy":"77"},{"ruleId":"68","replacedBy":"78"},{"ruleId":"70","severity":1,"message":"71","line":1,"column":17,"nodeType":"72","messageId":"73","endLine":1,"endColumn":26},{"ruleId":"66","replacedBy":"79"},{"ruleId":"68","replacedBy":"80"},"no-native-reassign",["81"],"no-negated-in-lhs",["82"],"no-unused-vars","'Component' is defined but never used.","Identifier","unusedVar","'NavbarBrand' is defined but never used.",["81"],["82"],["81"],["82"],["81"],["82"],"no-global-assign","no-unsafe-negation"]